    "debug": "^4.0.1",
    "got": "^11.5.2",
    "graphql": "^15.5.1",
    "jsdom": "^16.6.0",
    "luxon": "^2.0.1",
    "node-fetch": "^2.6.7",
    "p-map": "4",
    "puppeteer-core": "^10.1.0",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "serverless-offline": "^7.0.0",
    "x-ray": "^2.3.4"
  },
  "devDependencies": {
    "prettier": "^2.3.2",
