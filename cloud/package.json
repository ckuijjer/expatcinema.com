{
  "name": "expatcinema.com-cloud",
  "version": "1.0.1",
  "description": "",
  "bin": {
    "backend": "bin/backend.js"
  },
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "tsx": "IS_LOCAL=true node --inspect --no-warnings --import tsx",
    "type-check": "tsc --noEmit",
    "tail-cloudwatch-logs": "lumigo-cli tail-cloudwatch-logs -n /aws/lambda/expatcinema-dev --profile casper --region eu-west-1",
    "prettify-log": "awk -F'\\t' '{if ($5 && $5 ~ /^{/ && !system(\"echo \\x27\" $5 \"\\x27 | jq -e > /dev/null 2>&1\")) { print $1\"\\t\"$2\"\\t\"$3\"\\t\"$4 system(\"echo \\x27\" $5 \"\\x27 | jq\") } else { print $0 }}'",
    "prettify-log:test": "cat tail-cloudwatch-logs.example.log | pnpm prettify-log",
    "log": "pnpm tail-cloudwatch-logs|pnpm prettify-log",
    "install-chromium": "npx @puppeteer/browsers install chromium@latest --path /tmp/localChromium | awk '{print $2}' | xargs -I {} echo 'export const LOCAL_CHROMIUM_EXECUTABLE_PATH = `{}`;' > browser-local-constants.ts",
    "build": "tsc",
    "test": "jest",
    "bootstrap": "cdk bootstrap",
    "deploy:ci": "cdk deploy --require-approval never",
    "deploy": "AWS_PROFILE=casper STAGE=dev cdk deploy --hotswap-fallback",
    "synth": "AWS_PROFILE=casper STAGE=dev cdk synth",
    "watch": "AWS_PROFILE=casper STAGE=dev cdk watch --hotswap-fallback",
    "scrapers": "AWS_PROFILE=casper aws lambda invoke --function-name expatcinema-dev-scrapers output/expatcinema-dev-scrapers.json",
    "scrapers:prod": "AWS_PROFILE=casper aws lambda invoke --function-name expatcinema-prod-scrapers output/expatcinema-prod-scrapers.json"
  },
  "author": "Casper Kuijjer",
  "license": "ISC",
  "dependencies": {
    "@apollo/client": "^3.13.1",
    "@aws-lambda-powertools/logger": "^2.15.0",
    "@aws-sdk/client-dynamodb": "^3.758.0",
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/lib-dynamodb": "^3.758.0",
    "@middy/core": "^6.1.3",
    "@sparticuz/chromium": "^133.0.0",
    "aws-cdk-lib": "2.180.0",
    "camelcase-keys": "^9.1.3",
    "constructs": "^10.0.0",
    "diacritics": "^1.3.0",
    "got": "^14.4.6",
    "graphql": "^16.10.0",
    "html-entities": "^2.5.2",
    "leven": "^4.0.0",
    "luxon": "^3.5.0",
    "p-map": "7.0.3",
    "p-retry": "6.2.1",
    "puppeteer-core": "24.3.1",
    "react": "^19.0.0",
    "request": "^2.88.2",
    "source-map-support": "^0.5.21",
    "x-ray": "^2.3.4",
    "x-ray-crawler": "^2.0.5"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@tsconfig/node22": "^22.0.0",
    "@types/aws-lambda": "^8.10.147",
    "@types/diacritics": "^1.3.3",
    "@types/jest": "^29.5.14",
    "@types/luxon": "^3.4.2",
    "@types/node": "22.7.9",
    "@types/ramda": "^0.30.2",
    "@types/request": "^2.48.12",
    "@types/x-ray": "^2.3.6",
    "aws-cdk": "2.1001.0",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.0",
    "jest": "^29.7.0",
    "lumigo-cli": "^0.48.4",
    "prettier": "^3.5.2",
    "puppeteer": "24.3.1",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.3",
    "typescript": "5.8.2"
  }
}
