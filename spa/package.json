{
  "name": "expatcinema.com",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://www.expatcinema.com",
  "dependencies": {
    "axios": "^0.18.0",
    "emotion": "^9.2.8",
    "gh-pages": "^2.0.1",
    "luxon": "^1.6.2",
    "ramda": "^0.25.0",
    "react": "^16.7.0-alpha.0",
    "react-dom": "^16.7.0-alpha.0",
    "react-easy-params": "^2.0.2",
    "react-easy-state": "^6.0.5",
    "react-emotion": "^9.2.8",
    "react-ga": "^2.5.3",
    "react-scripts": "^2.1.1",
    "react-virtualized": "^9.20.1",
    "source-map-explorer": "^1.6.0"
  },
  "scripts": {
    "start": "yarn get-current-screenings && react-scripts start",
    "build": "yarn get-current-screenings && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "get-current-screenings": "node getCurrentScreenings",
    "analyze": "ls build/static/js/*js | xargs -I {} source-map-explorer {}"
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">0.25%",
      "not op_mini all",
      "ie 11"
    ]
  }
}
